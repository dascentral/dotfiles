#!/usr/bin/env bash

GREEN='\033[1;32m'
YELLOW='\033[1;33m'
WHITE='\033[1;37m'
NC='\033[0m'

###############################################################################

parts=(`echo $(git symbolic-ref -q HEAD) | tr '/' ' '`)

if [ $parts[3] != 'feature' ]; then
    printf "\033[1;33mYou do not appear to be within a feature branch.\033[0m\n"
else
    git hf feature finish ${parts[4]}
fi
