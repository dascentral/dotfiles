[
  // Cursor
  {
    "key": "cmd+l",
    "command": "expandLineSelection",
    "when": "textInputFocus"
  },
  {
    "key": "cmd+p",
    "command": "-expandLineSelection",
    "when": "textInputFocus"
  },
  // Better PHPUnit
  {
    "key": "cmd+t",
    "command": "better-phpunit.run"
  },
  {
    "key": "shift+cmd+t",
    "command": "better-phpunit.run-file"
  },
  // PHP Namespace Resolver
  {
    "key": "alt+cmd+i",
    "command": "namespaceResolver.import",
    "when": "editorTextFocus"
  },
]
