// Place your key bindings in this file to override the defaults
[
  {
    "key": "cmd+t",
    "command": "better-phpunit.run"
  },
  {
    "key": "shift+cmd+t",
    "command": "better-phpunit.run-file"
  },
  {
    "key": "alt+cmd+i",
    "command": "namespaceResolver.import",
    "when": "editorTextFocus"
  },
]
